{
  "name": "my-react-app",
  "version": "0.1.0",
  "private": true,
  "license": "ELv2",
  "dependencies": {
    "@capacitor/app": "^1.0.3",
    "@capacitor/cli": "^3.2.4",
    "@capacitor/core": "^3.2.4",
    "@capacitor/ios": "^3.2.4",
    "@capacitor/local-notifications": "^1.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ionicons": "^4.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start":              "BROWSER=edge craco start",
    "start-rs":           "BROWSER=edge react-scripts start",
    "dev":                "npm run start-rs",
    "build":              "BUILD_PATH='./build' craco build",
    "build-rs":           "BUILD_PATH='./build' react-scripts build",
    "build-gh":           "BUILD_PATH='./docs' craco build",
    "build-gh-rs":        "BUILD_PATH='./docs' react-scripts build",
    "build-all":          "npm run build && npm run build-gh",
    "build-all-rs":       "npm run build-rs && npm run build-gh-rs",
    "serve":              "npm run build && npx http-server ./build -p 3000",
    "serve-rs":           "npm run build-rs && npx http-server ./build -p 3000",
    "serve-gh":           "npm run build-gh && npx http-server ./docs -p 3000",
    "serve-gh-rs":        "npm run build-gh-rs && npx http-server ./docs -p 3000",
    "ios-sync":           "npx cap sync ios",
    "ios-copy":           "npx cap copy ios",
    "ios-run":            "npx cap run ios",
    "ios-open":           "npm run ios-copy && npx cap open ios",
    "build-ios-run":      "npm run build-all && npm run ios-run",
    "build-rs-ios-run":   "npm run build-all-rs && npm run ios-run",
    "build-ios-open":     "npm run build-all && npm run ios-open",
    "build-rs-ios-open":  "npm run build-all-rs && npm run ios-open",
    "git-add":            "git add **/**",
    "all":                "npm run build-all && npm run git-add",
    "all-rs":             "npm run build-all-rs && npm run git-add",
    "test":               "craco test",
    "test-rs":               "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "http-server": "^13.0.2",
    "typescript": "^4.4.3"
  }
}
